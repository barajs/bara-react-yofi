{
  "name": "bara-react-yofi",
  "description": "Collection of beautiful React components work with Bara cross-platform",
  "version": "1.2.1",
  "license": "MIT",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "private": false,
  "keywords": [
    "bara",
    "react",
    "react-native",
    "material",
    "beautiful",
    "ui",
    "ux",
    "typescript",
    "cross-platform"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "compile": "tsc -b ./tsconfig.lib.json",
    "compile:watch": "tsc -b ./tsconfig.lib.json -w",
    "compile:website": "cd website && yarn build",
    "start:website": "cd website && yarn start",
    "start:react": "react-scripts start",
    "start": "yarn compile && run-p compile:watch start:website",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build": "run-s compile compile:website",
    "build:react": "react-scripts build",
    "commit": "git-cz"
  },
  "dependencies": {
    "bara": "^2.4.0",
    "bara-react": "^3.1.0",
    "bara-react-icons": "^1.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-native-web": "0.11.4"
  },
  "devDependencies": {
    "@semantic-release/changelog": "3.0.2",
    "@semantic-release/git": "7.0.8",
    "@types/jest": "24.0.13",
    "@types/node": "11.13.13",
    "@types/react": "16.8.19",
    "@types/react-dom": "16.8.4",
    "@types/react-native": "0.57.62",
    "commitizen": "3.1.1",
    "cz-conventional-changelog": "2.1.0",
    "npm-run-all": "4.1.5",
    "react-scripts": "3.0.0",
    "semantic-release": "15.13.12",
    "typescript": "3.5.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
